{
  "name": "extensions-demo",
  "version": "0.0.1",
  "manifest_version": 2,
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js",
      "./reload.js"
    ]
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://localhost/*"
      ],
      "js": [
        "count.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://localhost/*"
      ],
      "js": [
        "count.js"
      ],
      "run_at": "document_end"
    }
  ]
}